# utils
alias prettyjson='python -m json.tool'
# git
alias git-merged-branches-show='git branch --merged | grep -v "\*" | grep -v master | xargs -n 1 echo'
alias git-merged-branches-delete='git branch --merged | grep -v "\*" | grep -v master | xargs -n 1 git branch -d'
alias git-recent-branches='git for-each-ref --sort=-committerdate refs/heads/'
